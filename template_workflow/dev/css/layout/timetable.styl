.teacher-timetable {
    width 100%
    text-align center
    font-family 'Montserrat'

    .time-cell {
        border 3px solid $white
        border-radius 5px
        width 12.4%
        background #efefef
    }

    .day-names {
        font-weight bold
    }

    .available-cell {
        width 100%
        height 100%
    }
}